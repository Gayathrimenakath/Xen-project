rver/lvroot
rd_NO_DM rhgb quiet
	initrd /initramfs-2.6.32-504.el6.x86_64.img
title Xen  with Linux 2.6.32.71 dom0
	root (hd0,0)
	kernel /xen-4.7.0.gz dom0_mem=3D512M loglvl=3Dall guest_loglvl=3Dall
	module /vmlinuz-2.6.32.71 ro root=3D/dev/mapper/vg_vmserver-lvroot
rd_LVM_LV=3Dvg_vmserver/lvroot nomodeset
	module /initrd-2.6.32.71.img=

